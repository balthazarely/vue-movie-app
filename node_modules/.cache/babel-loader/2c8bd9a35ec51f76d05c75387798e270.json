AAA,GAAA,CAAA,gBAAA,EADA,CAEA;;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,EAAA,CAAA,MAAA,CAAA,QAAA,EAHA,CAIA;AACA,GAnBA;AAoBA,EAAA,OApBA,qBAoBA;AAAA;;AACA,SAAA,SAAA,GADA,CAEA;;AACA,IAAA,KAAA,CACA,GADA,8CAEA,KAAA,OAFA,wDAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,MAAA,KAAA,CAAA,YAAA,GAAA,QAAA,CAAA,IAAA,CADA,CAEA;AACA,KAPA,EAHA,CAYA;;AACA,IAAA,KAAA,CACA,GADA,8CAEA,KAAA,OAFA,8EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,UAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,UAAA,MAAA,GAAA,KAAA,CAAA,MAAA,CAFA,CAGA;;AACA,MAAA,KAAA,CAAA,gBAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,gBAAA,EAAA,iBAAA;AACA,KAVA;AAWA,GA5CA;AA6CA,EAAA,OAAA,EAAA;AACA,IAAA,mBADA,+BACA,KADA,EACA,QADA,EACA,WADA,EACA;AACA,MAAA,EAAA,CAAA,UAAA,CAAA,gBAAA,EAAA,GAAA,CAAA;AACA,QAAA,KAAA,EAAA,QADA;AAEA,QAAA,OAAA,EAAA,KAFA;AAGA,QAAA,WAAA,EAAA;AAHA,OAAA;AAKA,KAPA;AAQA,IAAA,SARA,uBAQA;AAAA;;AACA,MAAA,KAAA,CACA,GADA,8CAEA,KAAA,OAFA,+DAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA,CADA,CAEA;AACA,OAPA;AAQA;AAjBA;AA7CA,CAAA","sourcesContent":["<template>\n  <section class=\"hero bg-container\">\n    <!-- <div class=\"hero overlay\"></div> -->\n    <div class=\"hero-body\" style=\"border: 2px solid blue\">\n      <div class=\"container\">\n        <h1 class=\"title\">\n          Hero title\n        </h1>\n        <div class=\"columns\" style=\"border: 2px solid red\">\n          <div class=\"column is-one-third\">\n            <figure class=\"image is-2-by-3\">\n              <img\n                class=\"movie-cover\"\n                v-bind:src=\"\n                  'https://image.tmdb.org/t/p/w200/' + movieData.poster_path\n                \"\n              />\n            </figure>\n          </div>\n          <div class=\"column is-two-thirds\">\n            <h1>{{ movieData.title }}</h1>\n            <h2>{{ movieData.tagline }}</h2>\n            <h2>{{ movieData.release_date }}</h2>\n            <h2>{{ movieData.runtime }}</h2>\n\n            <div class=\"genre\" v-for=\"(genre, i) in movieData.genres\" :key=\"i\">\n              <div>{{ genre.name }}</div>\n            </div>\n            <p>{{ movieData.overview }}</p>\n\n            <button\n              class=\"\"\n              @click=\"\n                addMovieToFavorites(\n                  movieData.id,\n                  movieData.title,\n                  movieData.poster_path\n                )\n              \"\n            >\n              Add to Favorites\n            </button>\n\n            <!-- need to make this so that this is a vloop  -->\n\n            <carousel :perPage=\"4\" class=\"carousel\">\n              <slide\n                class=\"carousel-slide\"\n                v-for=\"movie in similarMovieData\"\n                :key=\"movie.id\"\n              >\n                <img\n                  class=\"movie-cover\"\n                  v-bind:src=\"\n                    'https://image.tmdb.org/t/p/w200/' + movie.poster_path\n                  \"\n                />\n                <router-link\n                  :to=\"{\n                    name: 'Movie',\n                    params: { movie_id: movie.id },\n                  }\"\n                >\n                  {{ movie.title }}\n                </router-link>\n              </slide>\n            </carousel>\n\n            <!-- <div\n              class=\"cast\"\n              v-for=\"actor in movieCredits.cast\"\n              :key=\"actor.id\"\n            >\n              <div>{{ actor.name }}</div>\n            </div> -->\n          </div>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport \"bulma/css/bulma.css\";\nimport axios from \"axios\";\nimport db from \"@/firebase/init\";\nimport { Carousel, Slide } from \"vue-carousel\";\n\nexport default {\n  name: \"Movie\",\n  components: {\n    Carousel,\n    Slide,\n  },\n  data() {\n    return {\n      movieId: this.$route.params.movie_id,\n      movieData: [],\n      movieCredits: [],\n      similarMovieData: [],\n    };\n  },\n  beforeRouteUpdate(to, from, next) {\n    console.log(\"route uipdated\");\n    // this.movieId = to.path;\n    console.log(to.params.movie_id);\n    // this.fetchData();\n  },\n  created() {\n    this.fetchData();\n    // CREDITS\n    axios\n      .get(\n        `https://api.themoviedb.org/3/movie/${this.movieId}/credits?api_key=5e9bd2fa585826bdfc4233fb6424f425`\n      )\n      .then((response) => {\n        this.movieCredits = response.data;\n        // console.log(this.movieCredits, \"movie credits\");\n      });\n\n    // SIMILAR MOVIES\n    axios\n      .get(\n        `https://api.themoviedb.org/3/movie/${this.movieId}/similar?api_key=5e9bd2fa585826bdfc4233fb6424f425&language=en-US&page=1`\n      )\n      .then((response) => {\n        let array = response.data.results;\n        let length = array.length;\n        // this.similarMovieData = array.splice(14 + 1, array.length - (14 + 1));\n        this.similarMovieData = response.data.results;\n        console.log(this.similarMovieData, \" similar movies\");\n      });\n  },\n  methods: {\n    addMovieToFavorites(newId, newTitle, poster_path) {\n      db.collection(\"favoriteMovies\").add({\n        title: newTitle,\n        movieId: newId,\n        poster_path: poster_path,\n      });\n    },\n    fetchData() {\n      axios\n        .get(\n          `https://api.themoviedb.org/3/movie/${this.movieId}?api_key=5e9bd2fa585826bdfc4233fb6424f425&language=en-US`\n        )\n        .then((response) => {\n          this.movieData = response.data;\n          // console.log(this.movieData);\n        });\n    },\n  },\n};\n</script>\n\n<style lang=\"sass\">\n.container\n  position: relative\n\n.movie-cover\n  border-radius: 10px\n  margin: 10px\n  width: 120px\n\n.bg-container\n  // background-repeat: no-repeat\n  // background-size: cover\n  // background-position: center\n  // height: 100%\n  // width: 100%\n  // z-index: -0\n\n.overlay\n  width: 100%\n  height: 100%\n  background: red\n  position: absolute\n  top: 0\n  opacity: 0.5\n\n\n.carousel-slide\n\n.carousel\n  border: 2px solid red\n  height: 300px\n</style>\n"],"sourceRoot":"src/views"}]}