,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,QAAA,CAAA,IAAA,CAAA,WAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,YAAA;AACA,OATA;AAUA,KAbA;AAeA,IAAA,mBAfA,+BAeA,KAfA,EAeA,QAfA,EAeA,WAfA,EAeA;AACA,MAAA,EAAA,CAAA,UAAA,CAAA,gBAAA,EAAA,GAAA,CAAA;AACA,QAAA,KAAA,EAAA,QADA;AAEA,QAAA,OAAA,EAAA,KAFA;AAGA,QAAA,WAAA,EAAA,WAHA;AAIA,QAAA,SAAA,EAAA,IAAA,IAAA;AAJA,OAAA,EADA,CAOA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,KAxCA;AAyCA,IAAA,QAzCA,sBAyCA;AACA,WAAA,WAAA;AACA,WAAA,WAAA;AACA,KA5CA;AA6CA,IAAA,YA7CA,0BA6CA;AACA,WAAA,WAAA;AACA,WAAA,WAAA;AACA;AAhDA,GAdA;AAgEA,EAAA,QAAA,EAAA;AACA,IAAA,gBADA,8BACA;AACA,UAAA,KAAA,WAAA,IAAA,CAAA,EAAA;AACA,eAAA,IAAA;AACA,OAFA,MAEA;AACA,eAAA,KAAA;AACA;AACA,KAPA;AAQA,IAAA,YARA,0BAQA;AACA,UAAA,KAAA,WAAA,IAAA,KAAA,UAAA,EAAA;AACA,eAAA,IAAA;AACA,OAFA,MAEA;AACA,eAAA,KAAA;AACA;AACA;AAdA,GAhEA;AAgFA,EAAA,KAAA,EAAA;AACA,IAAA,QAAA,EAAA,kBAAA,GAAA,EAAA,MAAA,EAAA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA,EAAA,MAAA;AACA,WAAA,QAAA,GAAA,GAAA;AACA,MAAA,KAAA,CACA,GADA,uDAEA,KAAA,QAFA,gDAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,YAAA;AACA,OAPA;AAQA;AAZA;AAhFA,CAAA","sourcesContent":["<template>\n  <section class=\"hero\" style=\"background: red\">\n    <div class=\"hero-body\">\n      <div class=\"container\">\n        <h1 class=\"title\">\n          Hero title\n        </h1>\n        <h2 class=\"subtitle\">\n          Hero subtitle\n        </h2>\n\n        <div class=\"field\">\n          <label class=\"label\">Trending Timeframe</label>\n          <div class=\"control\">\n            <div class=\"select\">\n              <select v-model=\"filterBy\">\n                <option>day</option>\n                <option>week</option>\n              </select>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"columns\">\n          <div class=\"columns is-mobile is-multiline is-variable is-3\">\n            <div\n              class=\"column  is-one-fifth-widescreen is-one-quarter-desktop is-one-third-tablet is-one-third-mobile movie-card\"\n              v-for=\"movie in topMovieData.results\"\n              :key=\"movie.id\"\n            >\n              <button\n                class=\"button is-success add-movie-btn\"\n                @click=\"\n                  addMovieToFavorites(movie.id, movie.title, movie.poster_path)\n                \"\n              >\n                <span class=\"icon is-small\">\n                  <i class=\"fas fa-check\"></i>\n                </span>\n                <span>Save</span>\n              </button>\n\n              <figure class=\"image movie-image is-2-by-3\">\n                <img\n                  class=\"movie-cover\"\n                  v-bind:src=\"\n                    'https://image.tmdb.org/t/p/w200/' + movie.poster_path\n                  \"\n                />\n                <div class=\"movie-rating\">{{ movie.vote_average }}</div>\n              </figure>\n              <div class=\"title is-6 is-size-4-mobile movie-title\">\n                <router-link\n                  :to=\"{\n                    name: 'Movie',\n                    params: { movie_id: movie.id },\n                  }\"\n                >\n                  {{ movie.title }}\n                </router-link>\n              </div>\n              <div class=\"subtitle is-6 movie-subtitle\">\n                {{ movie.release_date }}\n              </div>\n              <div class=\"tags\">\n                <span class=\"tag\">One</span>\n              </div>\n            </div>\n          </div>\n        </div>\n        <nav class=\"pagination\" role=\"navigation\" aria-label=\"pagination\">\n          <a\n            class=\"pagination-previous\"\n            title=\"This is the first page\"\n            :disabled=\"previousDisabled\"\n            @click=\"previousPage\"\n            >Previous</a\n          >\n          <a class=\"pagination-next\" @click=\"nextPage\" :disabled=\"nextDisabled\"\n            >Next page</a\n          >\n        </nav>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport \"bulma/css/bulma.css\";\nimport axios from \"axios\";\nimport db from \"@/firebase/init\";\nexport default {\n  name: \"about\",\n  data() {\n    return {\n      topMovieData: [],\n      filterBy: \"day\",\n      apiKey: process.env.VUE_APP_API_KEY,\n      currentPage: 1,\n      totalPages: null,\n    };\n  },\n  mounted() {\n    this.fetchMovies();\n  },\n  methods: {\n    fetchMovies() {\n      // get most popular movies\n      axios\n        .get(\n          `https://api.themoviedb.org/3/movie/popular?api_key=${this.apiKey}&language=en-US&page=${this.currentPage}`\n          // `https://api.themoviedb.org/3/trending/movie/${this.filterBy}?api_key=${this.apiKey}&page=${this.currentPage}`\n        )\n        .then((response) => {\n          this.topMovieData = response.data;\n          this.totalPages = response.data.total_pages;\n          console.log(this.topMovieData);\n        });\n    },\n\n    addMovieToFavorites(newId, newTitle, poster_path) {\n      db.collection(\"favoriteMovies\").add({\n        title: newTitle,\n        movieId: newId,\n        poster_path: poster_path,\n        dateAdded: new Date(),\n      });\n      // const query = db\n      //   .collection(\"favoriteMovies\")\n      //   .where(\"movieId\", \"==\", newId)\n      //   .get()\n      //   .then((doc) => {\n      //     if (doc.docs.length >= 2) {\n      //       console.log(\"this already exisits\");\n      //     } else {\n      //       db.collection(\"favoriteMovies\").add({\n      //         title: newTitle,\n      //         movieId: newId,\n      //         poster_path: poster_path,\n      //         dateAdded: new Date(),\n      //       });\n      //     }\n      //   });\n\n      console.log(newId);\n    },\n    nextPage() {\n      this.currentPage++;\n      this.fetchMovies();\n    },\n    previousPage() {\n      this.currentPage--;\n      this.fetchMovies();\n    },\n  },\n  computed: {\n    previousDisabled() {\n      if (this.currentPage == 1) {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    nextDisabled() {\n      if (this.currentPage == this.totalPages) {\n        return true;\n      } else {\n        return false;\n      }\n    },\n  },\n  watch: {\n    filterBy: function(val, oldVal) {\n      console.log(val, oldVal);\n      this.filterBy = val;\n      axios\n        .get(\n          `https://api.themoviedb.org/3/trending/movie/${this.filterBy}?api_key=5e9bd2fa585826bdfc4233fb6424f425`\n        )\n        .then((response) => {\n          this.topMovieData = response.data;\n          console.log(this.topMovieData);\n        });\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}