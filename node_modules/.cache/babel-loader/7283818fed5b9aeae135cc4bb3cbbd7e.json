,6DAEA,KAAA,MAFA,kCAEA,KAAA,WAFA,4BAEA,KAAA,YAFA,oBAEA,KAAA,WAFA,GAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,IAAA,CAAA,KAAA,EAAA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,WAAA,GAAA,sCAAA;AACA,SAHA,MAGA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA;AACA,UAAA,KAAA,CAAA,UAAA,GAAA,QAAA,CAAA,IAAA,CAAA,WAAA;AACA,UAAA,KAAA,CAAA,iBAAA,GAAA,IAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,UAAA;AACA,UAAA,KAAA,CAAA,WAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA;AACA,OAhBA;AAiBA,KAnBA;AAoBA,IAAA,mBApBA,+BAoBA,KApBA,EAoBA,QApBA,EAoBA,WApBA,EAoBA;AACA,MAAA,EAAA,CAAA,UAAA,CAAA,gBAAA,EAAA,GAAA,CAAA;AACA,QAAA,KAAA,EAAA,QADA;AAEA,QAAA,OAAA,EAAA,KAFA;AAGA,QAAA,WAAA,EAAA,WAHA;AAIA,QAAA,SAAA,EAAA,IAAA,IAAA;AAJA,OAAA,EADA,CAQA;AACA;AACA;AACA,KA/BA;AAgCA,IAAA,QAhCA,sBAgCA;AACA,WAAA,WAAA;AACA,WAAA,eAAA;AACA,KAnCA;AAoCA,IAAA,YApCA,0BAoCA;AACA,WAAA,WAAA;AACA,WAAA,eAAA;AACA;AAvCA,GAfA;AAwDA,EAAA,QAAA,EAAA;AACA,IAAA,gBADA,8BACA;AACA,UAAA,KAAA,WAAA,IAAA,CAAA,EAAA;AACA,eAAA,IAAA;AACA,OAFA,MAEA;AACA,eAAA,KAAA;AACA;AACA,KAPA;AAQA,IAAA,YARA,0BAQA;AACA,UAAA,KAAA,WAAA,IAAA,KAAA,UAAA,EAAA;AACA,eAAA,IAAA;AACA,OAFA,MAEA;AACA,eAAA,KAAA;AACA;AACA;AAdA;AAxDA,CAAA","sourcesContent":["<template>\n  <div class=\"container is-fullhd\">\n    <div class=\"notification\">\n      <div class=\"columns\">\n        <div class=\"column is-narrow\">\n          <div class=\"box\">\n            <p class=\"title is-5\">Search</p>\n            <input\n              class=\"input\"\n              type=\"text\"\n              placeholder=\"Text input\"\n              v-on:keyup.enter=\"getMovieByTitle\"\n              v-model=\"movieSearch\"\n              label=\"Standard\"\n            />\n            <label class=\"checkbox\" style=\"margin-top: 10px\">\n              <input v-model=\"includeAdult\" type=\"checkbox\" />\n              Include Adult\n            </label>\n          </div>\n        </div>\n\n        <div class=\"columns is-multiline  is-mobile  is-variable is-3\">\n          <div\n            class=\"column  is-one-quarter-widescreen is-one-third-mobile is-one-third-desktop is-half-tablet  movie-card\"\n            v-for=\"movie in movieData.results\"\n            :key=\"movie.id\"\n          >\n            <button\n              class=\"button is-success add-movie-btn\"\n              @click=\"\n                addMovieToFavorites(movie.id, movie.title, movie.poster_path)\n              \"\n            >\n              <span class=\"icon is-small\">\n                <i class=\"fas fa-check\"></i>\n              </span>\n              <span>Save</span>\n            </button>\n\n            <figure class=\"image movie-image is-2-by-3\">\n              <router-link\n                :to=\"{\n                  name: 'Movie',\n                  params: { movie_id: movie.id },\n                }\"\n              >\n                <img\n                  class=\"movie-cover\"\n                  v-bind:src=\"\n                    'https://image.tmdb.org/t/p/w200/' + movie.poster_path\n                  \"\n                />\n              </router-link>\n\n              <div class=\"movie-rating\">{{ movie.vote_average }}</div>\n            </figure>\n            <div class=\"title is-6 is-size-4-mobile movie-title\">\n              <router-link\n                :to=\"{\n                  name: 'Movie',\n                  params: { movie_id: movie.id },\n                }\"\n              >\n                {{ movie.title }}\n              </router-link>\n            </div>\n            <div class=\"subtitle is-6 movie-subtitle\">\n              {{ movie.release_date }}\n            </div>\n            <div class=\"tags\">\n              <span class=\"tag\">One</span>\n            </div>\n          </div>\n        </div>\n      </div>\n      <nav\n        class=\"pagination\"\n        role=\"navigation\"\n        aria-label=\"pagination\"\n        v-if=\"hasSearchHappened\"\n      >\n        <a\n          class=\"pagination-previous\"\n          title=\"This is the first page\"\n          :disabled=\"previousDisabled\"\n          @click=\"previousPage\"\n          >Previous</a\n        >\n        <a class=\"pagination-next\" @click=\"nextPage\" :disabled=\"nextDisabled\"\n          >Next page</a\n        >\n      </nav>\n    </div>\n  </div>\n</template>\n\n<script>\nimport \"bulma/css/bulma.css\";\nimport axios from \"axios\";\nimport db from \"@/firebase/init\";\n\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      hasSearchHappened: false,\n      movieSearch: \"\",\n      includeAdult: false,\n      movieData: [],\n      topMovieData: [],\n      errFeedback: null,\n      apiKey: process.env.VUE_APP_API_KEY,\n      currentPage: 1,\n      totalPages: null,\n    };\n  },\n  methods: {\n    getMovieByTitle() {\n      axios\n        .get(\n          `https://api.themoviedb.org/3/search/movie?api_key=${this.apiKey}&language=en-US&page=${this.currentPage}&include_adult=${this.includeAdult}&query=${this.movieSearch}`\n        )\n        .then((response) => {\n          if (response.data.Error) {\n            this.movieData = null;\n            this.errFeedback = \"Your search didn't yeild any results\";\n          } else {\n            this.movieData = response.data;\n            this.totalPages = response.data.total_pages;\n            this.hasSearchHappened = true;\n            console.log(this.totalPages);\n            this.errFeedback = \"\";\n            console.log(response.data);\n          }\n        });\n    },\n    addMovieToFavorites(newId, newTitle, poster_path) {\n      db.collection(\"favoriteMovies\").add({\n        title: newTitle,\n        movieId: newId,\n        poster_path: poster_path,\n        dateAdded: new Date(),\n      });\n\n      // this.favoriteMovies.some((title) => title.id === newId)\n      //   ? null\n      //   : this.favoriteMovies.push({ id: newId, title: newTitle });\n    },\n    nextPage() {\n      this.currentPage++;\n      this.getMovieByTitle();\n    },\n    previousPage() {\n      this.currentPage--;\n      this.getMovieByTitle();\n    },\n  },\n  computed: {\n    previousDisabled() {\n      if (this.currentPage == 1) {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    nextDisabled() {\n      if (this.currentPage == this.totalPages) {\n        return true;\n      } else {\n        return false;\n      }\n    },\n  },\n};\n</script>\n\n<style lang=\"sass\">\n.movie-cover\n  border-radius: 10px\n\n.movie-title\n  text-align: left\n  margin-top: 10px\n\n.movie-subtitle\n  text-align: left\n  margin-top: 2px\n\n.fav-movie-cover\n  height: 60px\n  width: 40px\n  img\n    width: 100%\n    height: 100%\n\n.movie-card\n  position: relative\n  &:hover\n    .add-movie-btn\n       opacity: 1\n\n.add-movie-btn\n  position: absolute\n  top: 22px\n  right: 22px\n  z-index: 10\n  opacity: 0\n\n.movie-rating\n  border: 2px solid red\n  background: red\n  width: 40px\n  height: 40px\n  display: flex\n  justify-content: center\n  align-items: center\n  color: white\n  position: absolute\n  z-index: 10\n  right: 0px\n  bottom: 0px\n</style>\n"],"sourceRoot":"src/views"}]}