QAAA,QAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,cAAA,KAAA,GAAA;AACA,YAAA,KAAA,EAAA,GAAA,CAAA,IAAA,GAAA,KADA;AAEA,YAAA,EAAA,EAAA,GAAA,CAAA,EAFA;AAGA,YAAA,WAAA,EAAA,GAAA,CAAA,IAAA,GAAA,WAHA;AAIA,YAAA,OAAA,EAAA,GAAA,CAAA,IAAA,GAAA;AAJA,WAAA;;AAOA,UAAA,KAAA,CAAA,eAAA,CAAA,IAAA,CAAA,KAAA;AACA,SATA;AAUA,OAbA;AAcA;AAhBA;AAjBA,CAAA","sourcesContent":["<template>\n  <div class=\"container is-fullhd\">\n    <div class=\"movie-list\" v-for=\"movie in journaledMovies\" :key=\"movie.id\">\n      {{ movie.title }}\n      <!-- <button @click=\"lanuchModal(movie.title)\">OPEN MODAL</button> -->\n      <!-- <div class=\"modal\">\n        <div class=\"modal-background\"></div>\n        <div class=\"modal-content\"></div>\n        <button\n          class=\"modal-close is-large\"\n          @click=\"closeModal\"\n          aria-label=\"close\"\n        ></button>\n      </div> -->\n    </div>\n  </div>\n</template>\n\n<script>\nimport \"bulma/css/bulma.css\";\nimport axios from \"axios\";\nimport db from \"@/firebase/init\";\nimport StarRating from \"vue-star-rating\";\n\nexport default {\n  name: \"about\",\n  components: {\n    StarRating,\n  },\n  data() {\n    return {\n      journaledMovies: [],\n      apiKey: process.env.VUE_APP_API_KEY,\n      rating: null,\n    };\n  },\n  created() {\n    this.getAllJournalMovies();\n    console.log(this.journaledMovies);\n  },\n\n  methods: {\n    getAllJournalMovies() {\n      db.collection(\"movieJournal\")\n        .get()\n        .then((snapshot) => {\n          snapshot.forEach((doc) => {\n            const movie = {\n              title: doc.data().title,\n              id: doc.id,\n              poster_path: doc.data().poster_path,\n              movieId: doc.data().movieId,\n            };\n\n            this.journaledMovies.push(movie);\n          });\n        });\n    },\n  },\n};\n</script>\n\n<style lang=\"sass\">\n.movie-cover\n  border-radius: 10px\n\n.movie-title\n  text-align: left\n  margin-top: 10px\n\n.movie-subtitle\n  text-align: left\n  margin-top: 2px\n\n.fav-movie-cover\n  height: 60px\n  width: 40px\n  img\n    width: 100%\n    height: 100%\n\n.movie-card\n  position: relative\n  &:hover\n    .add-movie-btn\n       opacity: 1\n\n.add-movie-btn\n  position: absolute\n  top: 22px\n  right: 22px\n  z-index: 10\n  opacity: 0\n\n.movie-rating\n  border: 2px solid red\n  background: red\n  width: 40px\n  height: 40px\n  display: flex\n  justify-content: center\n  align-items: center\n  color: white\n  position: absolute\n  z-index: 10\n  right: 0px\n  bottom: 0px\n\n.rating-wrapper\n    border: 2px solid red\n</style>\n"],"sourceRoot":"src/views"}]}