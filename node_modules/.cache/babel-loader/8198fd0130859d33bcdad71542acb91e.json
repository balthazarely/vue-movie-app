AIA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,IAAA,CAAA,KAAA,EAAA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,WAAA,GAAA,sCAAA;AACA,SAHA,MAGA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA;AACA,UAAA,KAAA,CAAA,WAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA;AACA,OAbA;AAcA,KAhBA;AAiBA,IAAA,mBAjBA,+BAiBA,KAjBA,EAiBA,QAjBA,EAiBA;AACA,MAAA,EAAA,CAAA,UAAA,CAAA,gBAAA,EAAA,GAAA,CAAA;AACA,QAAA,KAAA,EAAA,QADA;AAEA,QAAA,EAAA,EAAA;AAFA,OAAA;AAIA,WAAA,cAAA,CAAA,IAAA,CAAA,UAAA,KAAA;AAAA,eAAA,KAAA,CAAA,EAAA,KAAA,KAAA;AAAA,OAAA,IACA,IADA,GAEA,KAAA,cAAA,CAAA,IAAA,CAAA;AAAA,QAAA,EAAA,EAAA,KAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAAA,CAFA;AAGA,KAzBA;AA2BA,IAAA,wBA3BA,oCA2BA,EA3BA,EA2BA;AAAA;;AACA;AACA,MAAA,EAAA,CAAA,UAAA,CAAA,gBAAA,EACA,GADA,CACA,EADA,EAEA,MAFA,GAGA,IAHA,CAGA,YAAA;AACA,QAAA,MAAA,CAAA,cAAA,GAAA,MAAA,CAAA,cAAA,CAAA,MAAA,CAAA,UAAA,KAAA,EAAA;AACA,iBAAA,KAAA,CAAA,EAAA,IAAA,EAAA;AACA,SAFA,CAAA;AAGA,OAPA;AAQA;AArCA,GAbA;AAoDA,EAAA,OApDA,qBAoDA;AAAA;;AACA,IAAA,EAAA,CAAA,UAAA,CAAA,gBAAA,EACA,GADA,GAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,MAAA,QAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,KAAA,GAAA,GAAA,CAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,EAAA;;AACA,QAAA,MAAA,CAAA,cAAA,CAAA,IAAA,CAAA,KAAA;AACA,OAJA;AAKA,KARA;AASA,IAAA,OAAA,CAAA,GAAA,CAAA,KAAA,cAAA;AACA;AA/DA,CAAA","sourcesContent":["<template>\n  <div class=\"container is-fullhd\">\n    <div class=\"notification\">\n      <div class=\"columns\">\n        <div class=\"column is-narrow\">\n          <div class=\"box\">\n            <p class=\"title is-5\">Search</p>\n            <input\n              class=\"input\"\n              type=\"text\"\n              placeholder=\"Text input\"\n              v-on:keyup.enter=\"getMovieByTitle\"\n              v-model=\"movieSearch\"\n              label=\"Standard\"\n            />\n            <label class=\"checkbox\" style=\"margin-top: 10px\">\n              <input v-model=\"includeAdult\" type=\"checkbox\" />\n              Include Adult\n            </label>\n          </div>\n          <div class=\"favorites\">\n            <h2>fav</h2>\n            <ul v-for=\"movie in favoriteMovies\" :key=\"movie.id\">\n              <router-link\n                :to=\"{\n                  name: 'Movie',\n                  params: { movie_id: movie.id },\n                }\"\n              >\n                {{ movie.title }}\n              </router-link>\n              <button @click=\"removeMovieFromFavorites(movie.docId)\">\n                X\n              </button>\n            </ul>\n          </div>\n        </div>\n\n        <div class=\"columns is-multiline is-variable is-3\">\n          <div\n            class=\"column  is-one-quarter-widescreen is-one-third-desktop is-half-tablet  movie-card\"\n            v-for=\"movie in movieData.results\"\n            :key=\"movie.id\"\n          >\n            <button\n              class=\"button is-success add-movie-btn\"\n              @click=\"addMovieToFavorites(movie.id, movie.title)\"\n            >\n              <span class=\"icon is-small\">\n                <i class=\"fas fa-check\"></i>\n              </span>\n              <span>Save</span>\n            </button>\n\n            <figure class=\"image movie-image is-2-by-3\">\n              <router-link\n                :to=\"{\n                  name: 'Movie',\n                  params: { movie_id: movie.id },\n                }\"\n              >\n                <img\n                  class=\"movie-cover\"\n                  v-bind:src=\"\n                    'https://image.tmdb.org/t/p/w200/' + movie.poster_path\n                  \"\n                />\n              </router-link>\n\n              <div class=\"movie-rating\">{{ movie.vote_average }}</div>\n            </figure>\n            <div class=\"title is-6 is-size-4-mobile movie-title\">\n              <router-link\n                :to=\"{\n                  name: 'Movie',\n                  params: { movie_id: movie.id },\n                }\"\n              >\n                {{ movie.title }}\n              </router-link>\n            </div>\n            <div class=\"subtitle is-6 movie-subtitle\">\n              {{ movie.release_date }}\n            </div>\n            <div class=\"tags\">\n              <span class=\"tag\">One</span>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport \"bulma/css/bulma.css\";\nimport axios from \"axios\";\nimport db from \"@/firebase/init\";\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      movieSearch: \"\",\n      includeAdult: false,\n      movieData: [],\n      topMovieData: [],\n      favoriteMovies: [],\n      errFeedback: null,\n      apiKey: \"c8ecd631\",\n    };\n  },\n  methods: {\n    getMovieByTitle() {\n      axios\n        .get(\n          `https://api.themoviedb.org/3/search/movie?api_key=5e9bd2fa585826bdfc4233fb6424f425&language=en-US&page=1&include_adult=${this.includeAdult}&query=${this.movieSearch}`\n        )\n        .then((response) => {\n          if (response.data.Error) {\n            this.movieData = null;\n            this.errFeedback = \"Your search didn't yeild any results\";\n          } else {\n            this.movieData = response.data;\n            this.errFeedback = \"\";\n            console.log(response.data);\n          }\n        });\n    },\n    addMovieToFavorites(newId, newTitle) {\n      db.collection(\"favoriteMovies\").add({\n        title: newTitle,\n        id: newId,\n      });\n      this.favoriteMovies.some((title) => title.id === newId)\n        ? null\n        : this.favoriteMovies.push({ id: newId, title: newTitle });\n    },\n\n    removeMovieFromFavorites(id) {\n      // Delete doc from firestore\n      db.collection(\"favoriteMovies\")\n        .doc(id)\n        .delete()\n        .then(() => {\n          this.favoriteMovies = this.favoriteMovies.filter((movie) => {\n            return movie.id != id;\n          });\n        });\n    },\n  },\n  created() {\n    db.collection(\"favoriteMovies\")\n      .get()\n      .then((snapshot) => {\n        snapshot.forEach((doc) => {\n          let movie = doc.data();\n          movie.docId = doc.id;\n          this.favoriteMovies.push(movie);\n        });\n      });\n    console.log(this.favoriteMovies);\n  },\n};\n</script>\n\n<style lang=\"sass\">\n.movie-cover\n  border-radius: 10px\n\n.movie-title\n  text-align: left\n  margin-top: 10px\n\n.movie-subtitle\n  text-align: left\n  margin-top: 2px\n\n.fav-movie-cover\n  height: 60px\n  width: 40px\n  img\n    width: 100%\n    height: 100%\n\n.movie-card\n  position: relative\n  &:hover\n    .add-movie-btn\n       opacity: 1\n\n.add-movie-btn\n  position: absolute\n  top: 22px\n  right: 22px\n  z-index: 10\n  opacity: 0\n\n.movie-rating\n  border: 2px solid red\n  background: red\n  width: 40px\n  height: 40px\n  display: flex\n  justify-content: center\n  align-items: center\n  color: white\n  position: absolute\n  z-index: 10\n  right: 0px\n  bottom: 0px\n</style>\n"],"sourceRoot":"src/views"}]}