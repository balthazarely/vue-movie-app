CA,QAAA,QAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,cAAA,KAAA,GAAA;AACA,YAAA,KAAA,EAAA,GAAA,CAAA,IAAA,GAAA,KADA;AAEA,YAAA,EAAA,EAAA,GAAA,CAAA,EAFA;AAGA,YAAA,WAAA,EAAA,GAAA,CAAA,IAAA,GAAA,WAHA;AAIA,YAAA,OAAA,EAAA,GAAA,CAAA,IAAA,GAAA,OAJA;AAKA,YAAA,MAAA,EAAA,GAAA,CAAA,IAAA,GAAA,MALA;AAMA,YAAA,UAAA,EAAA,GAAA,CAAA,IAAA,GAAA;AANA,WAAA;;AAQA,UAAA,KAAA,CAAA,eAAA,CAAA,IAAA,CAAA,KAAA;AACA,SAVA;AAWA,OAdA;AAeA,KAjBA;AAkBA,IAAA,sBAlBA,kCAkBA,EAlBA,EAkBA;AAAA;;AACA,WAAA,eAAA,GAAA,KAAA,eAAA,CAAA,MAAA,CAAA,UAAA,KAAA,EAAA;AACA,eAAA,KAAA,CAAA,OAAA,IAAA,EAAA;AACA,OAFA,CAAA;AAGA,MAAA,EAAA,CAAA,UAAA,CAAA,cAAA,EACA,GADA,CACA,EADA,cAGA,IAHA,CAGA,YAAA;AACA,QAAA,MAAA,CAAA,eAAA,GAAA,MAAA,CAAA,eAAA,CAAA,MAAA,CAAA,UAAA,KAAA,EAAA;AACA,iBAAA,KAAA,CAAA,EAAA,IAAA,EAAA;AACA,SAFA,CAAA;AAGA,OAPA;AAQA;AA9BA;AAjBA,CAAA","sourcesContent":["<template>\n  <div class=\"container is-fullhd\">\n    <div class=\"movie-list\" v-for=\"movie in journaledMovies\" :key=\"movie.id\">\n      {{ movie.title }}\n      <router-link\n        class=\"secondary-content\"\n        v-bind:to=\"{\n          name: 'journal-edit',\n          params: { id: movie.id },\n        }\"\n      >\n        <button>Edit</button></router-link\n      >\n      {{ movie.rating }}\n      <button @click=\"removeMovieFromJournal(movie.id)\">x</button>\n      <div class=\"my-comments\">{{ movie.myComments }}</div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport \"bulma/css/bulma.css\";\nimport axios from \"axios\";\nimport db from \"@/firebase/init\";\nimport StarRating from \"vue-star-rating\";\n\nexport default {\n  name: \"about\",\n  components: {\n    StarRating,\n  },\n  data() {\n    return {\n      journaledMovies: [],\n      apiKey: process.env.VUE_APP_API_KEY,\n      rating: null,\n    };\n  },\n  created() {\n    this.getAllJournalMovies();\n    console.log(this.journaledMovies);\n  },\n\n  methods: {\n    getAllJournalMovies() {\n      db.collection(\"movieJournal\")\n        .get()\n        .then((snapshot) => {\n          snapshot.forEach((doc) => {\n            const movie = {\n              title: doc.data().title,\n              id: doc.id,\n              poster_path: doc.data().poster_path,\n              movieId: doc.data().movieId,\n              rating: doc.data().rating,\n              myComments: doc.data().myComments,\n            };\n            this.journaledMovies.push(movie);\n          });\n        });\n    },\n    removeMovieFromJournal(id) {\n      this.journaledMovies = this.journaledMovies.filter((movie) => {\n        return movie.movieId != id;\n      });\n      db.collection(\"movieJournal\")\n        .doc(id)\n        .delete()\n        .then(() => {\n          this.journaledMovies = this.journaledMovies.filter((movie) => {\n            return movie.id != id;\n          });\n        });\n    },\n  },\n};\n</script>\n\n<style lang=\"sass\">\n.movie-cover\n  border-radius: 10px\n\n.movie-title\n  text-align: left\n  margin-top: 10px\n\n.movie-subtitle\n  text-align: left\n  margin-top: 2px\n\n.fav-movie-cover\n  height: 60px\n  width: 40px\n  img\n    width: 100%\n    height: 100%\n\n.movie-card\n  position: relative\n  &:hover\n    .add-movie-btn\n       opacity: 1\n\n.add-movie-btn\n  position: absolute\n  top: 22px\n  right: 22px\n  z-index: 10\n  opacity: 0\n\n.movie-rating\n  border: 2px solid red\n  background: red\n  width: 40px\n  height: 40px\n  display: flex\n  justify-content: center\n  align-items: center\n  color: white\n  position: absolute\n  z-index: 10\n  right: 0px\n  bottom: 0px\n\n.rating-wrapper\n    border: 2px solid red\n</style>\n"],"sourceRoot":"src/views"}]}