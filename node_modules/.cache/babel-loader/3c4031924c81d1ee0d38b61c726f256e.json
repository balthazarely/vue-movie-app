,GADA,CACA,EADA,cAGA,IAHA,CAGA,YAAA;AACA,QAAA,KAAA,CAAA,cAAA,GAAA,KAAA,CAAA,cAAA,CAAA,MAAA,CAAA,UAAA,KAAA,EAAA;AACA,iBAAA,KAAA,CAAA,EAAA,IAAA,EAAA;AACA,SAFA,CAAA;AAGA,OAPA;AAQA,KAbA;AAcA,IAAA,YAdA,wBAcA,KAdA,EAcA,QAdA,EAcA,WAdA,EAcA;AACA,MAAA,EAAA,CAAA,UAAA,CAAA,cAAA,EAAA,GAAA,CAAA;AACA,QAAA,KAAA,EAAA,QADA;AAEA,QAAA,OAAA,EAAA,KAFA;AAGA,QAAA,WAAA,EAAA;AAHA,OAAA;AAKA,KApBA;AAqBA,IAAA,oBArBA,kCAqBA;AAAA;;AACA,MAAA,EAAA,CAAA,UAAA,CAAA,gBAAA,EACA,GADA,GAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,QAAA,QAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,cAAA,KAAA,GAAA;AACA,YAAA,KAAA,EAAA,GAAA,CAAA,IAAA,GAAA,KADA;AAEA,YAAA,EAAA,EAAA,GAAA,CAAA,EAFA;AAGA,YAAA,WAAA,EAAA,GAAA,CAAA,IAAA,GAAA,WAHA;AAIA,YAAA,OAAA,EAAA,GAAA,CAAA,IAAA,GAAA,OAJA;AAKA,YAAA,KAAA,EAAA,GAAA,CAAA,IAAA,GAAA;AALA,WAAA;;AAQA,UAAA,MAAA,CAAA,cAAA,CAAA,IAAA,CAAA,KAAA;AACA,SAVA;AAWA,OAdA;AAeA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,cAAA;AACA;AAtCA;AAZA,CAAA","sourcesContent":["<template>\n  <div class=\"container is-fullhd\">\n    <div class=\"notification\">\n      <div class=\"columns\">\n        <div class=\"columns is-multiline is-variable is-3\">\n          <div\n            class=\"column is-one-fifth-desktop  movie-card\"\n            v-for=\"movie in favoriteMovies\"\n            :key=\"movie.id\"\n          >\n            <button @click=\"removeMovieFromFavorites(movie.id)\">\n              X\n            </button>\n            <button\n              @click=\"addToJournal(movie.id, movie.title, movie.poster_path)\"\n            >\n              Watched\n            </button>\n\n            <figure class=\"image movie-image is-2-by-3\">\n              <img\n                class=\"movie-cover\"\n                v-bind:src=\"\n                  'https://image.tmdb.org/t/p/w200/' + movie.poster_path\n                \"\n              />\n              <div class=\"movie-rating\">{{ movie.vote_average }}</div>\n            </figure>\n            <div class=\"title is-6 is-size-4-mobile movie-title\">\n              <router-link\n                :to=\"{\n                  name: 'Movie',\n                  params: { movie_id: movie.movieId },\n                }\"\n              >\n                {{ movie.title }}\n              </router-link>\n            </div>\n            <div class=\"subtitle is-6 movie-subtitle\">\n              {{ movie.release_date }}\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport \"bulma/css/bulma.css\";\nimport axios from \"axios\";\nimport db from \"@/firebase/init\";\nexport default {\n  name: \"about\",\n  data() {\n    return {\n      favoriteMovies: [],\n      selectedGenre: null,\n      apiKey: process.env.VUE_APP_API_KEY,\n    };\n  },\n  created() {\n    this.getAllFavoriteMovies();\n  },\n  methods: {\n    removeMovieFromFavorites(id) {\n      this.favoriteMovies = this.favoriteMovies.filter((movie) => {\n        return movie.movieId != id;\n      });\n      db.collection(\"favoriteMovies\")\n        .doc(id)\n        .delete()\n        .then(() => {\n          this.favoriteMovies = this.favoriteMovies.filter((movie) => {\n            return movie.id != id;\n          });\n        });\n    },\n    addToJournal(newId, newTitle, poster_path) {\n      db.collection(\"movieJournal\").add({\n        title: newTitle,\n        movieId: newId,\n        poster_path: poster_path,\n      });\n    },\n    getAllFavoriteMovies() {\n      db.collection(\"favoriteMovies\")\n        .get()\n        .then((snapshot) => {\n          snapshot.forEach((doc) => {\n            const movie = {\n              title: doc.data().title,\n              id: doc.id,\n              poster_path: doc.data().poster_path,\n              movieId: doc.data().movieId,\n              genre: doc.data().genre,\n            };\n\n            this.favoriteMovies.push(movie);\n          });\n        });\n      console.log(this.favoriteMovies);\n    },\n  },\n};\n</script>\n\n<style lang=\"sass\">\n.movie-cover\n  border-radius: 10px\n\n.movie-title\n  text-align: left\n  margin-top: 10px\n\n.movie-subtitle\n  text-align: left\n  margin-top: 2px\n\n.fav-movie-cover\n  height: 60px\n  width: 40px\n  img\n    width: 100%\n    height: 100%\n\n.movie-card\n  position: relative\n  &:hover\n    .add-movie-btn\n       opacity: 1\n\n.add-movie-btn\n  position: absolute\n  top: 22px\n  right: 22px\n  z-index: 10\n  opacity: 0\n\n.movie-rating\n  border: 2px solid red\n  background: red\n  width: 40px\n  height: 40px\n  display: flex\n  justify-content: center\n  align-items: center\n  color: white\n  position: absolute\n  z-index: 10\n  right: 0px\n  bottom: 0px\n</style>\n"],"sourceRoot":"src/views"}]}