eA,IAfA,EAeA;AACA,SAAA,OAAA,GAAA,EAAA,CAAA,MAAA,CAAA,QAAA;AACA,SAAA,SAAA;AACA,GAlBA;AAmBA,EAAA,OAnBA,qBAmBA;AAAA;;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,SAAA,SAAA,GAFA,CAGA;;AACA,IAAA,KAAA,CACA,GADA,8CAEA,KAAA,OAFA,8BAEA,KAAA,MAFA,GAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,MAAA,KAAA,CAAA,YAAA,GAAA,QAAA,CAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,YAAA,EAAA,eAAA;AACA,KAPA;AAQA,GA/BA;AAgCA,EAAA,OAAA,EAAA;AACA,IAAA,mBADA,+BACA,KADA,EACA,QADA,EACA,WADA,EACA;AACA,MAAA,EAAA,CAAA,UAAA,CAAA,gBAAA,EAAA,GAAA,CAAA;AACA,QAAA,KAAA,EAAA,QADA;AAEA,QAAA,OAAA,EAAA,KAFA;AAGA,QAAA,WAAA,EAAA;AAHA,OAAA;AAKA,KAPA;AAQA,IAAA,SARA,uBAQA;AAAA;;AACA,MAAA,KAAA,CACA,GADA,8CAEA,KAAA,OAFA,sBAEA,KAAA,MAFA,sBAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,SAAA;AACA,OAPA;AASA,MAAA,KAAA,CACA,GADA,8CAEA,KAAA,OAFA,8BAEA,KAAA,MAFA,6BAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,YAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,YAAA,MAAA,GAAA,KAAA,CAAA,MAAA,CAFA,CAGA;;AACA,QAAA,MAAA,CAAA,gBAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,gBAAA,EAAA,iBAAA;AACA,OAVA;AAWA;AA7BA;AAhCA,CAAA","sourcesContent":["<template>\n  <section class=\"hero bg-container\">\n    <!-- <div class=\"hero overlay\"></div> -->\n    <div\n      class=\"hero-body banner-image\"\n      :style=\"{\n        backgroundPosition: 'center',\n        backgroundRepeat: 'no-repeat',\n        backgroundSize: 'cover',\n        backgroundImage: `linear-gradient(rgba(255,255,255,.1), rgba(255,255,255,.1)), url(https://image.tmdb.org/t/p/w200/${movieData.backdrop_path}) `,\n      }\"\n    >\n      <!-- movieData.backdrop_path -->\n      <div class=\"container\">\n        <h1 class=\"title\">\n          Hero title\n        </h1>\n\n        <div class=\"columns\" style=\"border: 2px solid red\">\n          <div class=\"column is-one-third\">\n            <figure class=\"image is-2-by-3\">\n              <img\n                class=\"movie-cover\"\n                v-bind:src=\"\n                  'https://image.tmdb.org/t/p/w200/' + movieData.poster_path\n                \"\n              />\n            </figure>\n          </div>\n          <div class=\"column is-two-thirds\">\n            <h1>{{ movieData.title }}</h1>\n            <h2>{{ movieData.tagline }}</h2>\n            <h2>{{ movieData.release_date }}</h2>\n            <h2>{{ movieData.runtime }}</h2>\n\n            <div class=\"genre\" v-for=\"(genre, i) in movieData.genres\" :key=\"i\">\n              <div>{{ genre.name }}</div>\n            </div>\n            <p>{{ movieData.overview }}</p>\n\n            <button\n              class=\"\"\n              @click=\"\n                addMovieToFavorites(\n                  movieData.id,\n                  movieData.title,\n                  movieData.poster_path\n                )\n              \"\n            >\n              Add to Favorites\n            </button>\n\n            <!-- need to make this so that this is a vloop  -->\n\n            <carousel :perPage=\"4\" class=\"carousel\">\n              <slide\n                class=\"carousel-slide\"\n                v-for=\"movie in similarMovieData\"\n                :key=\"movie.id\"\n              >\n                <img\n                  class=\"movie-cover\"\n                  v-bind:src=\"\n                    'https://image.tmdb.org/t/p/w200/' + movie.poster_path\n                  \"\n                />\n                <router-link\n                  :to=\"{\n                    name: 'Movie',\n                    params: { movie_id: movie.id },\n                  }\"\n                >\n                  {{ movie.title }}\n                </router-link>\n              </slide>\n            </carousel>\n\n            <!-- <div\n              class=\"cast\"\n              v-for=\"actor in movieCredits.cast\"\n              :key=\"actor.id\"\n            >\n              <div>{{ actor.name }}</div>\n            </div> -->\n          </div>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport \"bulma/css/bulma.css\";\nimport axios from \"axios\";\nimport db from \"@/firebase/init\";\nimport { Carousel, Slide } from \"vue-carousel\";\n\nexport default {\n  name: \"Movie\",\n  components: {\n    Carousel,\n    Slide,\n  },\n  data() {\n    return {\n      apiKey: process.env.VUE_APP_API_KEY,\n      movieId: this.$route.params.movie_id,\n      movieData: [],\n      movieCredits: [],\n      similarMovieData: [],\n    };\n  },\n  beforeRouteUpdate(to, from, next) {\n    this.movieId = to.params.movie_id;\n    this.fetchData();\n  },\n  created() {\n    console.log(\"hi\");\n    this.fetchData();\n    // CREDITS\n    axios\n      .get(\n        `https://api.themoviedb.org/3/movie/${this.movieId}/credits?api_key=${this.apiKey}`\n      )\n      .then((response) => {\n        this.movieCredits = response.data;\n        console.log(this.movieCredits, \"movie credits\");\n      });\n  },\n  methods: {\n    addMovieToFavorites(newId, newTitle, poster_path) {\n      db.collection(\"favoriteMovies\").add({\n        title: newTitle,\n        movieId: newId,\n        poster_path: poster_path,\n      });\n    },\n    fetchData() {\n      axios\n        .get(\n          `https://api.themoviedb.org/3/movie/${this.movieId}?api_key=${this.apiKey}&language=en-US`\n        )\n        .then((response) => {\n          this.movieData = response.data;\n          console.log(this.movieData);\n        });\n\n      axios\n        .get(\n          `https://api.themoviedb.org/3/movie/${this.movieId}/similar?api_key=${this.apiKey}&language=en-US&page=1`\n        )\n        .then((response) => {\n          let array = response.data.results;\n          let length = array.length;\n          // this.similarMovieData = array.splice(14 + 1, array.length - (14 + 1));\n          this.similarMovieData = response.data.results;\n          console.log(this.similarMovieData, \" similar movies\");\n        });\n    },\n  },\n};\n</script>\n\n<style lang=\"sass\">\n.container\n  position: relative\n\n.movie-cover\n  border-radius: 10px\n  margin: 10px\n  width: 120px\n\n.hero-body\n\n\n.overlay\n  width: 100%\n  height: 100%\n  background: red\n  position: absolute\n  top: 0\n  opacity: 0.5\n\n\n.carousel-slide\n\n.carousel\n  border: 2px solid red\n  height: 300px\n</style>\n"],"sourceRoot":"src/views"}]}